  <DataSets>
    <DataSet Name="AusPropertyAnalysis_Adv_RMReport">
      <Query>
        <DataSourceName>AustraliaPropertyAnalysis_Advanced_RMReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH RM AS(
select R.[Postcode], R.[Suburb], R.[City], R.[State], R.[StateCode], [RentalMedian], DG.GeographyKey, DG.Lat, DG.Lon
from [dbo].[Stg_Rental] AS R
LEFT JOIN [dbo].[DimGeography] AS DG
ON R.Postcode = DG.Postcode AND R.Suburb = DG.Suburb)
SELECT
DISTINCT CONCAT (G.[Suburb],'/', G.[City]) AS Location_of_Selected,
CONCAT (RM.[Suburb],'/', RM.[City]) AS Location_of_Property,
ROUND((geography::Point(G.[Lat],G.[Lon],4326).STDistance(geography::Point(RM.[Lat],RM.[Lon],4326)))/1000,3) AS DistanceInKM,
RM.[RentalMedian]
FROM RM
CROSS JOIN  [dbo].[DimGeography] AS G
WHERE ((geography::Point(G.[Lat],G.[Lon],4326).STDistance(geography::Point(RM.[Lat],RM.[Lon],4326)))/1000) &lt;= 1
AND G.Lat IS NOT NULL
AND G.Lon IS NOT NULL
AND RM.Lat IS NOT NULL
AND RM.Lon IS NOT NULL
AND RM.Suburb &lt;&gt; G.Suburb AND RM.Suburb IS NOT NULL 
AND CONCAT (G.[Suburb],'/', G.[City]) = @Location
ORDER BY CONCAT (RM.[Suburb],'/', RM.[City]) </CommandText>
      </Query>
      <Fields>
        <Field Name="Location_of_Selected">
          <DataField>Location_of_Selected</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Location_of_Property">
          <DataField>Location_of_Property</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DistanceInKM">
          <DataField>DistanceInKM</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="RentalMedian">
          <DataField>RentalMedian</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Filter_Location">
      <Query>
        <DataSourceName>Filter_Location</DataSourceName>
        <CommandText>select CONCAT (DG.[Suburb],'/', DG.[City]) AS Location_of_Selected
from [dbo].[Stg_Rental] AS R
LEFT JOIN [dbo].[DimGeography] AS DG
ON R.Postcode = DG.Postcode AND R.Suburb = DG.Suburb
WHERE CONCAT (DG.[Suburb],'/', DG.[City]) != '/'
ORDER BY CONCAT (DG.[Suburb],'/', DG.[City])</CommandText>
      </Query>
      <Fields>
        <Field Name="Location_of_Selected">
          <DataField>Location_of_Selected</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
