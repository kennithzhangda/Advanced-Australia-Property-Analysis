CONCAT (G.[Suburb],'/', G.[City]) AS Location_of_Selected
FROM [dbo].[DimTransport] AS T
CROSS JOIN  [dbo].[DimGeography] AS G
WHERE ((geography::Point(G.[Lat],G.[Lon],4326).STDistance(geography::Point(T.[Lat],T.[Lon],4326)))/1000) &lt;= 1
AND G.Lat IS NOT NULL
AND G.Lon IS NOT NULL
AND T.Lat IS NOT NULL
AND T.Lon IS NOT NULL
AND T.Suburb &lt;&gt; G.Suburb AND T.Suburb IS NOT NULL </CommandText>
      </Query>
      <Fields>
        <Field Name="Location_of_Selected">
          <DataField>Location_of_Selected</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
