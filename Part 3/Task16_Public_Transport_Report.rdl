  <DataSets>
    <DataSet Name="Public_Transport">
      <Query>
        <DataSourceName>Task16_Public_Transport_Report</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
Distinct [StopName],
CONCAT (G.[Suburb],'/', G.[City]) AS Location_of_Selected,
[Mode],
ROUND((geography::Point(G.[Lat],G.[Lon],4326).STDistance(geography::Point(T.[Lat],T.[Lon],4326)))/1000,3) AS DistanceInKM
FROM [dbo].[DimTransport] AS T
CROSS JOIN  [dbo].[DimGeography] AS G
WHERE ((geography::Point(G.[Lat],G.[Lon],4326).STDistance(geography::Point(T.[Lat],T.[Lon],4326)))/1000) &lt;= 1
AND G.Lat IS NOT NULL
AND G.Lon IS NOT NULL
AND T.Lat IS NOT NULL
AND T.Lon IS NOT NULL
AND T.Suburb &lt;&gt; G.Suburb AND T.Suburb IS NOT NULL
AND [StopName] NOT LIKE '**%'
AND CONCAT (G.[Suburb],'/', G.[City]) = @Location</CommandText>
      </Query>
      <Fields>
        <Field Name="StopName">
          <DataField>StopName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Location_of_Selected">
          <DataField>Location_of_Selected</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Mode">
          <DataField>Mode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DistanceInKM">
          <DataField>DistanceInKM</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Filter_Location">
      <Query>
