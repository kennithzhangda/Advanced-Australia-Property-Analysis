        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>b32b8354-a841-4cc2-a98f-30c3332192cc</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Local_School">
      <Query>
        <DataSourceName>Task17_Local_School_Report</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
Distinct [SchoolName],
CONCAT (G.[Suburb],'/', G.[City]) AS Location_of_Selected,
[Street] AS Address,[LevelofSchooling],
ROUND((geography::Point(G.[Lat],G.[Lon],4326).STDistance(geography::Point(S.[Lat],S.[Lon],4326)))/1000,3) AS DistanceInKM
FROM [dbo].[DimAuLocalSchool] AS S
CROSS JOIN  [dbo].[DimGeography] AS G
WHERE ((geography::Point(G.[Lat],G.[Lon],4326).STDistance(geography::Point(S.[Lat],S.[Lon],4326)))/1000) &lt;= 1
AND G.Lat IS NOT NULL
AND G.Lon IS NOT NULL
AND S.Lat IS NOT NULL
AND S.Lon IS NOT NULL
AND S.Suburb &lt;&gt; G.Suburb AND S.Suburb IS NOT NULL
AND CONCAT (G.[Suburb],'/', G.[City]) = @Location</CommandText>
      </Query>
      <Fields>
        <Field Name="SchoolName">
          <DataField>SchoolName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Location_of_Selected">
          <DataField>Location_of_Selected</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Address">
          <DataField>Address</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LevelofSchooling">
          <DataField>LevelofSchooling</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DistanceInKM">
          <DataField>DistanceInKM</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Filter_Location">
      <Query>
        <DataSourceName>Task17_Local_School_Report</DataSourceName>
        <CommandText>SELECT CONCAT (G.[Suburb],'/', G.[City]) AS Location_of_Selected
FROM [dbo].[DimAuLocalSchool] AS S
LEFT JOIN [dbo].[DimGeography] AS G
ON S.Suburb = G.Suburb AND S.Postcode = G.Postcode
WHERE CONCAT (G.[Suburb],'/', G.[City]) != '/'</CommandText>
      </Query>
      <Fields>
        <Field Name="Location_of_Selected">
          <DataField>Location_of_Selected</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
