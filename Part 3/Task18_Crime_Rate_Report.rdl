        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=DESKTOP-087M78G\IC;Initial Catalog=Australia_PropertyAnalysis_Advanced</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>f5d68af2-adab-4308-a44f-9ece340656d0</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Crime_Rate">
      <Query>
        <DataSourceName>Task18_Crime_Rate_Report</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
[OffenceCategory],
CAST(SUM(IncidentNumber) AS float)/ CAST(SUM(CY.Population) AS float) AS CrimeRate,
CONCAT (G.[Suburb],'/', G.[City]) AS Location_of_Selected,
[Year],
ROUND((geography::Point(G.[Lat],G.[Lon],4326).STDistance(geography::Point(G1.[Lat],G1.[Lon],4326)))/1000,3) AS DistanceInKM
FROM [dbo].[FactCrimeByYear] AS CY
LEFT JOIN [dbo].[DimGeography] AS G1
ON CY.[Suburb] = G1.Suburb AND CY.[Postcode] = G1.Postcode
LEFT JOIN [dbo].[DimOffenceCategory] AS OC
ON CY.OffenceCategoryKey = OC.OffenceCategoryKey
CROSS JOIN  [dbo].[DimGeography] AS G
WHERE ((geography::Point(G.[Lat],G.[Lon],4326).STDistance(geography::Point(G1.[Lat],G1.[Lon],4326)))/1000) &lt;= 1
AND G.Lat IS NOT NULL
AND G.Lon IS NOT NULL
AND G1.Lat IS NOT NULL
AND G1.Lon IS NOT NULL
AND G1.Suburb &lt;&gt; G.Suburb AND G1.Suburb IS NOT NULL
GROUP BY CAST(IncidentNumber AS float)/ CAST(CY.Population AS float), 
CONCAT (G.[Suburb],'/', G.[City]),[Year], ROUND((geography::Point(G.[Lat],G.[Lon],4326).STDistance(geography::Point(G1.[Lat],G1.[Lon],4326)))/1000,3), CY.Population, [OffenceCategory]
HAVING CY.Population != 0
AND CONCAT (G.[Suburb],'/', G.[City]) = @Location</CommandText>
      </Query>
      <Fields>
        <Field Name="OffenceCategory">
          <DataField>OffenceCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CrimeRate">
          <DataField>CrimeRate</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Location_of_Selected">
          <DataField>Location_of_Selected</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DistanceInKM">
          <DataField>DistanceInKM</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Filter_Location">
      <Query>
        <DataSourceName>Task18_Crime_Rate_Report</DataSourceName>
        <CommandText>SELECT CONCAT (G.[Suburb],'/', G.[City]) AS Location_of_Selected
FROM [dbo].[FactCrimeByYear] AS CY
LEFT JOIN [dbo].[DimGeography] AS G
ON CY.GeographyKey = G.GeographyKey</CommandText>
      </Query>
      <Fields>
        <Field Name="Location_of_Selected">
          <DataField>Location_of_Selected</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
